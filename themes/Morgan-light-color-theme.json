    "editorBracketPairGuide.background2": "#2d7c4a20",
    "editorBracketPairGuide.background3": "#47663b20",
    "editorBracketPairGuide.background4": "#1976D220",
    "editorBracketPairGuide.background5": "#5e88c720",
